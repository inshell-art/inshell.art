{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "../../node_modules/.tmp/tsconfig.home.jest.tsbuildinfo",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "typeRoots": ["../../node_modules/@types", "./tests"],
    "types": ["jest", "node", "@testing-library/jest-dom"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@inshell/*": ["../../packages/*/src"]
    }
  },
  "include": [
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "./jest-dom.d.ts"
  ]
}
