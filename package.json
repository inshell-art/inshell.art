{
  "name": "inshell.art",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode prod",
    "test": "jest",
    "emu": "npm run build && firebase emulators:start --only hosting",
    "wait:emu": "npx wait-on http://127.0.0.1:5002 && echo Emulator is ready",
    "e2e:emu": "NODE_ENV=emu cypress run --config-file cypress/cypress.config.ts",
    "deploy:staging": "firebase use inshell-art-staging && npm run build && firebase deploy --only hosting --token $FIREBASE_TOKEN",
    "wait:staging": "npx wait-on https://inshell-art-staging.web.app && echo Staging is ready",
    "e2e:staging": "NODE_ENV=staging cypress run --config-file cypress/cypress.config.ts",
    "deploy:prod": "firebase use inshell-art-prod && npm run build:prod && firebase deploy --only hosting --token $FIREBASE_TOKEN",
    "wait:prod": "npx wait-on https://inshell-art-prod.web.app && echo Production is ready",
    "e2e:prod": "NODE_ENV=prod cypress run --config-file cypress/cypress.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.22.0",
    "@emotion/react": "^11.14.0",
    "@fontsource/source-code-pro": "^5.2.6",
    "@starknet-react/core": "^3.7.4",
    "@tanstack/react-query": "^5.83.0",
    "@types/d3": "^7.4.3",
    "@uidotdev/usehooks": "^2.4.1",
    "@visx/axis": "^3.12.0",
    "@visx/curve": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "d3": "^7.9.0",
    "d3-scale": "^4.0.2",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.6",
    "msw": "^2.10.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "starknet": "^7.6.4",
    "vite": "^7.0.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/cypress": "^1.1.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.7",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "cypress": "^13.13.1",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "picomatch": "^4.0.3",
    "ts-jest": "^29.2.2",
    "typescript": "^5.8.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}