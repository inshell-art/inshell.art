{
  "name": "inshell-art",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev:home": "pnpm --filter @inshell/home dev",
    "dev:home:devnet": "pnpm --filter @inshell/home dev:devnet",
    "dev:thought": "pnpm --filter @inshell/thought dev",
    "build:home": "pnpm --filter @inshell/home build",
    "build:home:prod": "pnpm --filter @inshell/home build:prod",
    "build:thought": "pnpm --filter @inshell/thought build",
    "test": "pnpm --filter @inshell/home test",
    "test:fast": "pnpm --filter @inshell/home test:fast",
    "test:unit": "pnpm --filter @inshell/home test:unit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run type-check && pnpm run test:unit",
    "preview:home": "pnpm --filter @inshell/home preview",
    "e2e:staging": "NODE_ENV=staging cypress run --config-file cypress/cypress.config.ts",
    "e2e:prod": "NODE_ENV=prod cypress run --config-file cypress/cypress.config.ts",
    "prepare": "husky",
    "abi-gen": "npx abi-wan-kanabi --input packages/contracts/src/abi/PulseAuction.contract_class.json --output packages/contracts/src/generated/PulseAuction.abi.ts",
    "contract:build": "cd contracts/pulse && scarb build",
    "postcontract:build": "node scripts/copyAbi.js && npx abi-wan-kanabi --input packages/contracts/src/generated/pulse_PulseAuction.contract_class.json --output packages/contracts/src/generated/pulse_PulseAuction.abi.ts",
    "predeploy:contract": "pnpm run contract:build",
    "deploy:contract": "pnpm tsx scripts/deploy.ts",
    "pulse:update": "git submodule update --init --recursive",
    "postpulse:update": "pnpm -C contracts/pulse exec scarb build && npx abi-wan-kanabi --input contracts/pulse/target/dev/pulse_PulseAuction.contract_class.json --output packages/contracts/src/generated/PulseAuction.abi.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@types/node": "^24.2.0",
    "abi-wan-kanabi": "^2.2.4",
    "cypress": "^13.13.1",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "starknet": "^7.6.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "overrides": {
      "axios@^1.0.0": "1.13.2",
      "glob@^10.0.0": "10.5.0",
      "js-yaml@^3.0.0": "3.14.2",
      "js-yaml@^4.0.0": "4.1.1",
      "qs@^6.0.0": "6.14.1",
      "tmp@^0.2.0": "0.2.5"
    }
  }
}
